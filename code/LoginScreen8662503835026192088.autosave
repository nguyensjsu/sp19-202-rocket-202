public class LoginScreen implements IProxy{
	KeyPad kp;
	PinSet ks;
	IDisplayComponent chain;

  public LoginScreen(){
    kp = new KeyPad();
	  ks = new PinSet();
	  chain = ks;
	  ks.setNext(kp);
  }
   
  public void display(){
      chain.display();
    }
    
    public void touch(){
      chain.touch();
    }
	
}
