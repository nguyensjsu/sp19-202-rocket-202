
public class AppProxy {
  boolean authenticated;
  Login login;
  AppController app;
  
  public AppProxy() {
    authenticated = false;
    login = new Login();
    
  }
  
  public void display() {
    if(!authenticated) login.display();
    else frame.display();
  }
  
  public void touch(){
    if(!authenticated){
        login.touch();
    }
  }
}
